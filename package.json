{
  "name": "cash",
  "main": "expo-router/entry",
  "version": "1.2.0",
  "scripts": {
    "postinstall": "bunx tailwindcss -i ./src/global.css -o ./node_modules/.cache/nativewind/global.css",
    "prebuild": "bunx expo prebuild --clean",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android:staging": "bunx eas build --profile staging --platform android --output sipkl-mobile-staging.apk --local",
    "build:ios:staging": "bunx eas build --profile staging --platform ios --output sipkl-mobile-staging.ipa --local",
    "build:android:production": "bunx eas build --profile production --platform android --output sipkl-mobile.apk --local",
    "build:ios:production": "bunx eas build --profile production --platform ios --output sipkl-mobile.ipa --local",
    "update:staging": "cross-env EXPO_PUBLIC_APP_ENV=staging bunx eas update --channel staging --clear-cache",
    "update:production": "cross-env EXPO_PUBLIC_APP_ENV=production bunx eas update --channel production --clear-cache",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "test": "jest",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^6.0.2",
    "@tanstack/react-query": "^5.51.1",
    "axios": "^1.7.7",
    "axios-curlirize": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "expo": "~51.0.28",
    "expo-constants": "~16.0.2",
    "expo-font": "~12.0.9",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-updates": "~0.25.23",
    "expo-web-browser": "~13.0.3",
    "firebase": "^10.12.3",
    "lucide-react-native": "^0.453.0",
    "nativewind": "^4.1.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-dropdown-select-list": "^2.0.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-ui-datepicker": "^2.0.4",
    "react-native-uuid": "^2.0.2",
    "react-native-web": "~0.19.10",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/axios-curlirize": "^1.3.5",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.0.7",
    "cross-env": "^7.0.3",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.4",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
